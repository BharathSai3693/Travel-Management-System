<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head.ejs') %> <%- include('partials/adminnavbar.ejs') %>
  <body>
    <div
      class="bg-warning p-3 text-center"
      style="
        background-image: url('data:image/jpeg;base64,');
        background-size: cover;
      "
    >
      <h2 class="text-white">Administration</h2>
    </div>

    <div class="row">
      <div class="col-3" id="dbleftpart">
        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/adminusers" method="get">
            <input
              class="w-100 h-100 text-white h5 dashboarditems"
              type="submit"
              value="Users"
            />
          </form>
        </div>

        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/adminpackages" method="get">
            <input
              class="w-100 h-100 dashboarditems h5 text-white"
              type="submit"
              value="Packages"
            />
          </form>
        </div>

        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/package/new" method="get">
            <input
              class="dashboarditems h5 text-white w-100 h-100"
              type="submit"
              value="Create New Package"
            />
          </form>
        </div>

        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/adminbookings" method="get">
            <input
              class="dashboarditems h5 text-white w-100 h-100"
              type="submit"
              value="Bookings"
            />
          </form>
        </div>

        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/adminenquiries" method="get">
            <input
              class="dashboarditems h5 text-white w-100 h-100"
              type="submit"
              value="Enquiries"
            />
          </form>
        </div>

        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/adminreports" method="get">
            <input
              class="dashboarditems h5 text-white w-100 h-100"
              type="submit"
              value="Reports"
            />
          </form>
        </div>

        <div class="row" id="dashboarditems">
          <form class="w-100 h-100" action="/adminlogout" method="post">
            <input
              class="dashboarditems h5 text-white w-100 h-100"
              type="submit"
              value="Logout"
            />
          </form>
        </div>
      </div>

      <div
        class="col-9 p-4 bg-secondary"
        id="dbrightpart"
        style="
          background-image: url('https://img.freepik.com/free-vector/blue-pink-halftone-background_53876-99004.jpg?size=626&ext=jpgx');
          background-size: cover;
        "
      >
        <div class="row p-3">
          <div class="col-4 p-3 rounded m-auto">
            <a href="/adminusers" style="text-decoration: none">
              <div
                class="row p-3"
                style="
                  background-image: url('https://media.istockphoto.com/photos/abstract-background-in-gradient-in-yellow-and-orange-picture-id495798818?k=20&m=495798818&s=612x612&w=0&h=s6JUNR9jP8EV5bDf3gn_rAGvbjY3wz4Jl75dcoNQQ0g=');
                  background-size: cover;
                "
              >
                <div class="col m-auto">
                  <h1 class="text-center text-white"><%= usersCount %></h1>
                  <p class="text-center text-white h4">Users</p>
                </div>
                <div class="col m-auto">
                  <img
                    src="./images/team.png"
                    alt=""
                    style="height: 100px; width: 100px"
                  />
                </div>
              </div>
            </a>
          </div>

          <div class="col-4 p-3 rounded m-auto">
            <a href="/adminpackages" style="text-decoration: none;">
              <div
                class="row p-3"
                style="
                  background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRdaURQhQv05BbGIOzPAY-XWpZ9K77l0lLJHQ&usqp=CAU');
                  background-size: cover;
                "
              >
                <div class="col m-auto">
                  <h1 class="text-center text-white"><%= packagesCount %></h1>
                  <p class="text-center text-white h4">Packages</p>
                </div>
                <div class="col m-auto">
                  <img
                    src="./images/travel.png"
                    style="height: 100px; width: 100px"
                  />
                </div>
              </div>
            </a>
          </div>

          <div class="col-4 p-3 rounded m-auto">
            <a href="/adminbookings" style="text-decoration: none;">
            <div
              class="row p-3"
              style="
                background-image: url('data:image/jpeg;base64,');
                background-size: cover;
              "
            >
              <div class="col m-auto">
                <h1 class="text-center text-white"><%= bookingsCount %></h1>
                <p class="text-center text-white h4">Bookings</p>
              </div>
              <div class="col m-auto">
                <img
                  src="./images/booking.png"
                  alt=""
                  style="height: 100px; width: 100px"
                />
              </div>
            </div>
          </div>
        </a>
        </div>

        <div class="row p-3">
          <div class="col-4 p-3 rounded m-auto">
            <a href="/adminenquiries" style="text-decoration: none;">
            <div
              class="row p-3"
              style="
                background-image: url('data:image/jpeg;base64,');
                background-size: cover;
              "
            >
              <div class="col m-auto">
                <h1 class="text-center text-white"><%= enquiriesCount %></h1>
                <p class="text-center text-white h4">Enquiries</p>
              </div>
              <div class="col m-auto">
                <img
                  src="./images/team.png"
                  alt=""
                  style="height: 100px; width: 100px"
                />
              </div>
            </div>
          </a>
          </div>

          <div class="col-4 p-3 rounded m-auto">
            <a href="/adminreports" style="text-decoration: none;">
            <div
              class="row p-3"
              style="
                background-image: url('data:image/jpeg;base64,');
                background-size: cover;
              "
            >
              <div class="col m-auto">
                <p class="text-center text-white h4">Reports</p>
              </div>
              <div class="col m-auto">
                <img
                  src="./images/report.png"
                  alt=""
                  style="height: 100px; width: 100px"
                />
              </div>
            </div>
            </a>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>

