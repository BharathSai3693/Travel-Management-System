<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs") %> <%- include("./partials/header.ejs") %>
  <body
    style="
      background-image: url('data:image/jpeg;base64,');
      background-size: cover;
    "
  >
    <div class="bookings container my-5">
      <div class="row p-3 rounded yellow-grad">
        <div class="col-2 h5 text-light">Package Name</div>
        <div class="col-2 h5 text-light">Booking ID</div>
        <div class="col-2 h5 text-light">Date</div>
        <div class="col-3 h5 text-light">Email</div>
        <div class="col-3 h5 text-light">status</div>
      </div>

      <% for(let [index,b] of bookings.entries()) {%>
      <div class="row">
        <div class="col-2 text-break text-white h4">
          <%=packages[index].packageName%>
        </div>
        <div class="col-2 text-break text-white"><%=b._id%></div>
        <div class="col-2 text-break text-white">
          <%= b.bookedtime.toISOString().substring(0, 10) %>
        </div>
        <div class="col-3 text-white"><%= b.bookingMail %></div>

        <% var stat = ""; var color = "" %> <% if(b.status == 0) {stat = "Pending";color = "text-warning" } %> <%
        if(b.status == 1) {stat = "Confirmed";color = "text-success" } %> <% if(b.status == -1) {stat =
        "Rejected"; color = "text-danger" } %>
        <div class="col-3 h5 <%=color %>" ><%= stat %></div>
      </div>
      <hr color="white"
        size="10" > </hr> <% } %>
    </div>
  </body>
</html>
